#include "main.h"

/**
 * times_table -prints tables
 *
 * Return: Always 0.
 */
void jack_bauer(void)
{
	int ho, mi;

	for (ho = 00; ho < 24; ho++)
	{
		for (mi = 00; mi < 60; mi++)
		{
			if (ho < 24 && mi < 60)
			{
				_putchar((ho / 10) + '0');
				_putchar((ho % 10) + '0');
				_putchar(':');
				_putchar((mi % 10) + '0');
				_putchar('\n');
			}
		}
	}
/*	return (0);*/
}
